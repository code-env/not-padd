{
  "name": "notpadd",
  "description": "Notpadd your buildtime/runtime cms with your own data",
  "private": false,
  "version": "2.2.4",
  "type": "module",
  "main": "dist/index.cjs",
  "author": "Bossadi Zenith <hello@bossadizenith.me>",
  "license": "MIT",
  "homepage": "https://notpadd.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-env/not-padd.git",
    "directory": "packages/notpadd"
  },
  "bugs": "https://github.com/code-env/not-padd/issues",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
