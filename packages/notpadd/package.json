    "test": "vitest --run --coverage"
  },
  "devDependencies": {
    "@notpadd/eslint-config": "workspace:^",
    "@notpadd/typescript-config": "workspace:^",
    "@types/node": "^22.13.4",
    "next": "^14.2.32",
    "ts-node": "^10.9.2",
    "tsdown": "^0.15.6",
    "typescript": "^5"
  },
  "dependencies": {
    "notpadd-integrations": "workspace:^"
  },
  "peerDependencies": {
    "notpadd-core": "workspace:^",
    "next": "^12 || ^13 || ^14 || ^15"
